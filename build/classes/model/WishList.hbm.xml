<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<hibernate-mapping>
  <class name="model.WishList" table="wish_lists">
    <id column="item_id" name="itemId" type="int">
      <generator class="native" />
    </id>
    <property name="userId" column="user_id" type="int" />
    <property name="itemCategory" column="item_category" type="string" />
    <property name="itemName" column="item_name" type="string" />
    <property name="createDate" column="create_date" type="date" />
  </class>
</hibernate-mapping>